<div id="<%= dom_id mood_detection %>">
  <p>
    <strong>User name:</strong>
    <%= mood_detection.user_name %>
  </p>

  <p>
    <strong>Mood:</strong>
    <%= mood_detection.mood %>
  </p>

  <p>
    <strong>Confidence:</strong>
    <%= mood_detection.confidence %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= link_to mood_detection.image.filename, mood_detection.image if mood_detection.image.attached? %>
  </p>

</div>
